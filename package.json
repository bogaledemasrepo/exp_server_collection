  },
  "dependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "ts-node-dev": "^2.0.0"
